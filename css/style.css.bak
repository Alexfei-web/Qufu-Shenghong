.product_info li{ width:100%; list-style:none; display:block; height:20px; line-height:20px; font-size:12px; color:#333333; font-family:"宋体";overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.product_info li a{font-size:12px; color:#333333; text-decoration:none;}
.product_info li a:hover{text-decoration:underline;}

.b_s14{font-weight:bold; font-size:14px; color:#333333;}
