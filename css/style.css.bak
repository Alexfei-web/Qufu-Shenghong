.cc{margin-top:5px;}
.product_list a{display:block;padding-left:30px;padding-top:14px; padding-bottom:9px; text-align:left; background:url(../images/menu_link.gif) 0 55%;  background-repeat:no-repeat; font:"宋体"; font-size:12px;background-position-y:50%; font-weight:bold;  color:#838B8E; text-decoration:none;}
.product_list a:hover{padding-left:45px; background:url(../images/menu_hover.gif) no-repeat 0 55%;}
.title2{border:1px solid #d0cfcf; height:180px; margin-top:20px; background:url(../images/five_10.gif) no-repeat left top; text-align:left;line-height:35px;}
.title211{font-size:12px; font-weight:bold; color:#565656;margin-left:40px;}

.s_contact{margin:5px 5px 5px 15px; font-size:12px; font-family:"宋体"; color:#333; line-height:30px;}



.md_right{width:725px; float:left; background:url(../images/hh_16.jpg) no-repeat left top; overflow:hidden;}
.md_right_left{ padding-top:8px; padding-left:16px;}
