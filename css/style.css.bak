.rightnews_list a{color:#333; display:block; margin-left:15px; text-decoration:none; overflow:hidden; text-overflow:ellipsis;white-space:nowrap;width:15em; }
.rightnews_list a:hover{text-decoration:underline;color:#FF6600;}

.md_right_show{padding-left:16px; padding-right:8px; clear:both; padding-top:10px;}


